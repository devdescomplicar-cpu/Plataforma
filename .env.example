# Database
DATABASE_URL="postgresql://descompliCAR_user:descompliCAR_pass_2026@localhost:5432/descompliCAR"

# MinIO
MINIO_ENDPOINT="localhost"
MINIO_PORT=9000
MINIO_ACCESS_KEY="minioadmin"
MINIO_SECRET_KEY="minioadmin"
MINIO_BUCKET="vehicle-images"
MINIO_USE_SSL=false

# JWT
JWT_SECRET="your-secret-key-change-in-production"
JWT_EXPIRES_IN="7d"

# Server (produção: front+back porta 3000; dev: back 3002, front Vite 3001)
PORT=3000
NODE_ENV=development

# Base URL da plataforma (links em e-mails: reset-password, planos, etc.).
# OBRIGATÓRIO em produção - use o domínio real (ex: https://descomplicar.pratiko.app.br)
VITE_APP_URL="http://localhost:3000"

# API URL. Produção: deixe vazio (front e back na mesma porta, API em /api = sem CORS).
# Dev: vazio = Vite faz proxy /api → backend (porta 3002; configurável via VITE_PROXY_TARGET).
# Dev contra API remota: use a URL (ex: https://apidc.pratiko.app.br/api).
# Nesse caso, a API remota precisa incluir localhost no CORS_ORIGINS (ver docs/CORS-DEPLOY.md).
VITE_API_URL=

# Dev only: porta do Vite (default 3001). Produção não usa.
# VITE_DEV_PORT=3001
# Dev only: target do proxy /api (default http://127.0.0.1:3002). Use 127.0.0.1 para evitar EADDRNOTAVAIL.
# VITE_PROXY_TARGET=http://127.0.0.1:3002

# Push (PWA) - configurar no servidor (server/.env). Gerar: npx web-push generate-vapid-keys
# VAPID_PUBLIC_KEY=
# VAPID_PRIVATE_KEY=
# VAPID_MAILTO=mailto:support@seudominio.com

# E-mail (notificações admin) - senha SMTP (SmtpConfig no admin usa host/port/user; senha vem daqui)
# SMTP_PASSWORD=
